[configuration]

entry_symbol = "godot2d_dungeon_generation_library_init"
compatibility_minimum = "4.5"
reloadable=true

[libraries]
; Relative paths ensure that our GDExtension can be placed anywhere in the project directory.

windows.debug.x86_64 = "./bin/windows/Godot2D-DungeonGenerationCpp.windows.template_debug.x86_64.dll"
windows.release.x86_64 = "./bin/windows/Godot2D-DungeonGenerationCpp.windows.template_release.x86_64.dll"
windows.debug.x86_32 = "./bin/windows/Godot2D-DungeonGenerationCpp.windows.template_debug.x86_32.dll"
windows.release.x86_32 = "./bin/windows/Godot2D-DungeonGenerationCpp.windows.template_release.x86_32.dll"

linux.debug.x86_64 = "./bin/linux/libGodot2D-DungeonGenerationCpp.linux.template_debug.x86_64.so"
linux.release.x86_64 = "./bin/linux/libGodot2D-DungeonGenerationCpp.linux.template_release.x86_64.so"
linux.debug.arm64 = "./bin/linux/libGodot2D-DungeonGenerationCpp.linux.template_debug.arm64.so"
linux.release.arm64 = "./bin/linux/libGodot2D-DungeonGenerationCpp.linux.template_release.arm64.so"
linux.debug.rv64 = "./bin/linux/libGodot2D-DungeonGenerationCpp.linux.template_debug.rv64.so"
linux.release.rv64 = "./bin/linux/libGodot2D-DungeonGenerationCpp.linux.template_release.rv64.so"

macos.debug = "./bin/macos/libGodot2D-DungeonGenerationCpp.macos.template_debug.dylib"
macos.release = "./bin/macos/libGodot2D-DungeonGenerationCpp.macos.template_release.dylib"

android.debug.x86_64 = "./bin/android/libGodot2D-DungeonGenerationCpp.android.template_debug.x86_64.so"
android.release.x86_64 = "./bin/android/libGodot2D-DungeonGenerationCpp.android.template_release.x86_64.so"
android.debug.arm64 = "./bin/android/libGodot2D-DungeonGenerationCpp.android.template_debug.arm64.so"
android.release.arm64 = "./bin/android/libGodot2D-DungeonGenerationCpp.android.template_release.arm64.so"

ios.debug = "./bin/ios/libGodot2D-DungeonGenerationCpp.ios.template_debug.arm64.dylib"
ios.release = "./bin/ios/libGodot2D-DungeonGenerationCpp.ios.template_release.arm64.dylib"

web.debug.wasm32 = "./bin/web/libGodot2D-DungeonGenerationCpp.web.template_debug.wasm32.nothreads.wasm"
web.release.wasm32 = "./bin/web/libGodot2D-DungeonGenerationCpp.web.template_release.wasm32.nothreads.wasm"
